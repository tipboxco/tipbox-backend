model DMRequest {
  id                 String                 @id @default(uuid()) @db.Uuid
  fromUserId         String                 @map("from_user_id") @db.Uuid
  toUserId           String                 @map("to_user_id") @db.Uuid
  status             DMRequestStatus        @default(PENDING)
  type               SupportType            @default(GENERAL)
  amount             Float                  @default(0)
  description        String?                @db.Text
  threadId           String?                @map("thread_id") @db.Uuid
  fromUserRating     Int?                   @map("from_user_rating")
  toUserRating       Int?                   @map("to_user_rating")
  closedByFromUserAt DateTime?              @map("closed_by_from_user_at")
  closedByToUserAt   DateTime?              @map("closed_by_to_user_at")
  sentAt             DateTime               @default(now()) @map("sent_at")
  respondedAt        DateTime?              @map("responded_at")
  createdAt          DateTime               @default(now()) @map("created_at")
  updatedAt          DateTime               @updatedAt @map("updated_at")
  fromUser           User                   @relation("DMRequestSender", fields: [fromUserId], references: [id], onDelete: Cascade)
  toUser             User                   @relation("DMRequestReceiver", fields: [toUserId], references: [id], onDelete: Cascade)
  thread             DMThread?              @relation("DMRequestThread", fields: [threadId], references: [id], onDelete: SetNull)
  reports            SupportRequestReport[]

  @@index([fromUserId, toUserId])
  @@index([fromUserId])
  @@index([toUserId])
  @@index([status])
  @@index([threadId])
  @@map("dm_requests")
}

model SupportRequestReport {
  id          String   @id @default(uuid()) @db.Uuid
  requestId   String   @map("request_id") @db.Uuid
  reporterId  String   @map("reporter_id") @db.Uuid
  category    String
  description String?  @db.Text
  createdAt   DateTime @default(now()) @map("created_at")

  request  DMRequest @relation(fields: [requestId], references: [id], onDelete: Cascade)
  reporter User      @relation("SupportRequestReportReporter", fields: [reporterId], references: [id], onDelete: Cascade)

  @@index([requestId])
  @@index([reporterId])
  @@map("support_request_reports")
}

model DMSupportSession {
  id          String       @id @default(uuid()) @db.Uuid
  threadId    String       @map("thread_id") @db.Uuid
  helperId    String       @map("helper_id") @db.Uuid
  tipsAmount  Float        @map("tips_amount")
  supportedAt DateTime     @default(now()) @map("supported_at")
  createdAt   DateTime     @default(now()) @map("created_at")
  updatedAt   DateTime     @updatedAt @map("updated_at")
  feedbacks   DMFeedback[]
  helper      User         @relation(fields: [helperId], references: [id], onDelete: Cascade)
  thread      DMThread     @relation(fields: [threadId], references: [id], onDelete: Cascade)

  @@index([threadId])
  @@index([helperId])
  @@map("dm_support_sessions")
}

model DMFeedback {
  id          String           @id @default(uuid()) @db.Uuid
  sessionId   String           @map("session_id") @db.Uuid
  rating      Int
  comment     String?
  submittedAt DateTime         @default(now()) @map("submitted_at")
  createdAt   DateTime         @default(now()) @map("created_at")
  updatedAt   DateTime         @updatedAt @map("updated_at")
  session     DMSupportSession @relation(fields: [sessionId], references: [id], onDelete: Cascade)

  @@map("dm_feedbacks")
}

model Feed {
  id        String      @id @db.VarChar(26)
  userId    String      @map("user_id") @db.Uuid
  postId    String      @map("post_id") @db.VarChar(26)
  source    FeedSource
  seen      Boolean     @default(false)
  createdAt DateTime    @default(now()) @map("created_at")
  updatedAt DateTime    @updatedAt @map("updated_at")
  post      ContentPost @relation(fields: [postId], references: [id], onDelete: Cascade)
  user      User        @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId])
  @@index([userId, seen])
  @@index([userId, source])
  @@index([postId])
  @@map("feeds")
}

model FeedHighlight {
  id            String              @id @db.VarChar(26)
  postId        String              @map("post_id") @db.VarChar(26)
  reason        FeedHighlightReason
  highlightedAt DateTime            @default(now()) @map("highlighted_at")
  createdAt     DateTime            @default(now()) @map("created_at")
  updatedAt     DateTime            @updatedAt @map("updated_at")
  post          ContentPost         @relation(fields: [postId], references: [id], onDelete: Cascade)

  @@index([postId])
  @@map("feed_highlights")
}

model TrendingPost {
  id           String      @id @db.VarChar(26)
  postId       String      @map("post_id") @db.VarChar(26)
  score        Float
  trendPeriod  TrendPeriod @map("trend_period")
  calculatedAt DateTime    @default(now()) @map("calculated_at")
  createdAt    DateTime    @default(now()) @map("created_at")
  updatedAt    DateTime    @updatedAt @map("updated_at")
  post         ContentPost @relation(fields: [postId], references: [id], onDelete: Cascade)

  @@unique([postId, trendPeriod])
  @@map("trending_posts")
}

