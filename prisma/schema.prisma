model ModerationAction {
  id           String               @id @default(uuid()) @db.Uuid
  moderatorId  String               @map("moderator_id") @db.Uuid
  targetUserId String               @map("target_user_id") @db.Uuid
  reason       String
  actionType   ModerationActionType @map("action_type")
  contentType  String?              @map("content_type")
  contentId    Int?                 @map("content_id")
  createdAt    DateTime             @default(now()) @map("created_at")
  updatedAt    DateTime             @updatedAt @map("updated_at")
  moderator    User                 @relation("ModeratorActions", fields: [moderatorId], references: [id], onDelete: Cascade)
  targetUser   User                 @relation("ModeratedActions", fields: [targetUserId], references: [id], onDelete: Cascade)

  @@index([moderatorId])
  @@index([targetUserId])
  @@map("moderation_actions")
}

model ManualReviewFlag {
