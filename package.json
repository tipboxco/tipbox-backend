{
  "name": "tipbox-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "dotenv -e .env -- node dist/interfaces/server.js",
    "start:prod": "dotenv -e .env.production -- node dist/interfaces/server.js",
    "start:test": "dotenv -e .env.test -- node dist/interfaces/server.js",
    "worker": "ts-node src/infrastructure/workers/index.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:all": "jest --config jest.config.test.ts && node scripts/open-test-report.js detailed-test-report.html",
    "test:auth": "jest --config tests/jest-config/jest.config.auth.ts && node scripts/open-test-report.js auth-report.html",
    "test:user": "jest --config tests/jest-config/jest.config.user.ts && node scripts/open-test-report.js user-report.html",
    "test:user-settings": "jest --config tests/jest-config/jest.config.user-settings.ts && node scripts/open-test-report.js user-settings-report.html",
    "test:expert": "jest --config tests/jest-config/jest.config.expert.ts && node scripts/open-test-report.js expert-report.html",
    "test:feed": "jest --config tests/jest-config/jest.config.feed.ts && node scripts/open-test-report.js feed-report.html",
    "test:explore": "jest --config tests/jest-config/jest.config.explore.ts && node scripts/open-test-report.js explore-report.html",
    "test:inventory": "jest --config tests/jest-config/jest.config.inventory.ts && node scripts/open-test-report.js inventory-report.html",
    "test:marketplace": "jest --config tests/jest-config/jest.config.marketplace.ts && node scripts/open-test-report.js marketplace-report.html",
    "test:report": "node scripts/serve-test-report.js",
    "postinstall": "echo 'Skipping build for Docker'",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:user": "ts-node prisma/seed/user.seed.ts",
    "db:migrate": "npx prisma migrate dev --name init --create-only || npx prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tipboxco/tipbox-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tipboxco/tipbox-backend/issues"
  },
  "homepage": "https://github.com/tipboxco/tipbox-backend#readme",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^9.0.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-html-reporter": "^3.10.2",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.11.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/s3-request-presigner": "^3.658.1",
    "@prisma/client": "^6.19.0",
    "@socket.io/redis-adapter": "^8.2.1",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.3",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.62.0",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "google-auth-library": "^10.4.2",
    "googleapis": "^164.1.0",
    "helmet": "^8.1.0",
    "jest": "^30.2.0",
    "jest-html-reporters": "^3.1.7",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "prom-client": "^15.1.3",
    "redis": "^4.6.12",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ulid": "^3.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
